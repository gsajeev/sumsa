{
  "activationEvents": [
    "onLanguage:xml"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "xmlAnnotator.annotateFiles",
        "title": "Annotate XML Errors in Files"
      },
      {
        "command": "xmlAnnotator.removeDecorations",
        "title": "Clear Decorations"
      }
    ]
  },
  "name": "sumsa",
  "displayName": "sumsa",
  "description": "Annotates files with SA error from an XML file.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "eslint": "^9.13.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}